{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["prettier", "googleappsscript"],
  "env": {
    "googleappsscript/googleappsscript": true
  },
  "globals": {
    "CardService": true,
    "Gmail": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "no-console": 0
  }
}